
.row.header
  .col-xs-12
    i.fa.fa-2x.fa-times.detail-close(data-click='close')

  .col-xs-12.m-t-1
    h4.card-title= data['@id'].split('/').pop()

if views.img
  .row
    .col-xs-12.text-center
      img.img-thumbnail(data-zoom=views.img, src=views.img, style='max-height:30rem;')

.row.m-t-1
  .col-xs-12
    table.table.table-responsive.attr-table
      include ./table
